        "tags":[
          "SAST"
        ],
        "summary":"Unregisters an Engine Server",
        "operationId":"EngineServersV01_DeleteByid",
        "consumes":[
          "application/json;v=0.1"
        ],
        "produces":[
          "application/json;v=0.1"
        ],
        "parameters":[
          {
            "name":"id",
            "in":"path",
            "description":"Unique Id of the specific Engine Server",
            "required":true,
            "type":"integer",
            "format":"int64"
          }
        ],
        "responses":{
          "204":{
            "description":"Deleted",
            "schema":{
              "type":"object"
            }
          },
          "400":{
            "description":"Bad Request"
          },
          "404":{
            "description":"Not Found"
          }
        }
      }
    },
